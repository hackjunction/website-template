{
  "connection": "default",
  "collectionName": "staticcontent",
  "info": {
    "name": "staticcontent",
    "description": "Various static content on the website, which doesn't make sense as a standalone content type."
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "Public": {
      "default": true,
      "unique": true,
      "type": "boolean"
    },
    "HomePageTitle": {
      "default": "",
      "type": "string"
    },
    "HomePageBody": {
      "default": "",
      "type": "text"
    },
    "HomePageHeaderImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "NotFoundPageTitle": {
      "default": "",
      "type": "string"
    },
    "NotFoundPageBody": {
      "default": "",
      "type": "text"
    },
    "NotFoundPageHeaderImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}